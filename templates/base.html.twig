<!DOCTYPE html>
<html lang="en">
	<head>
		{% block head %}
			{# @@include("./partials/title-meta.html", {"title": "Sales Dashboard"}) #}
			{% include 'partials/title-meta.html.twig' %}
			<link href="{{ asset('build/assets/libs/admin-resources/jquery.vectormap/jquery-jvectormap-1.2.2.css') }}" rel="stylesheet" type="text/css"/>
			{% include 'partials/head-css.html.twig' %}
		{% endblock %}
	</head>

	{% include 'partials/body.html.twig' %}

	{% block body %}
		<div id="wrapper">
			{% include 'partials/menu.html.twig' %}
			<div class="content-page">
				<div class="content">
					<div class="container-fluid">
						{% block content %}{% endblock %}
					</div>
				</div>
			</div>
		</div>

		{% include 'partials/right-sidebar.html.twig' %}

		{% block javascript %}
			<!-- Vendor js -->
			<script src="{{ asset('build//assets/js/vendor.min.js') }}"></script>
			<!-- KNOB JS -->
			<script src="{{ asset('build/assets/libs/jquery-knob/jquery.knob.min.js') }}"></script>
			<!-- App js -->
			<script src="{{ asset('build/assets/js/app.min.js') }}"></script>
		{% endblock %}
		
	{% endblock %}
	</body>
</html>