{% extends "./base.html.twig" %}
{% block login %}{% endblock %}
{% form_theme form 'bootstrap_5_layout.html.twig' %}
{% block body %}
	<div class="container">
		<div class="d-flex flex-column">
			<div class="align-self-center" style="margin-top: 150px">
				{{ form_start(form) }}
				{{ form_widget(form) }}
				<button type="submit" class="btn btn-primary rounded-pill px-3 align-self-center">Upload</button>
				{{ form_end(form) }}
			</div>
		</div>
	</div>
{% endblock %}